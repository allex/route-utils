{
  "name": "route-utils",
  "version": "2.0.5",
  "description": "Vue route utilities (based on vue)",
  "jsnext:main": "lib/route-utils.esm.js",
  "module": "lib/route-utils.esm.js",
  "main": "lib/route-utils.js",
  "types": "lib/index.d.ts",
  "repository": "allex/route-utils",
  "keywords": [
    "traverse",
    "route-utils"
  ],
  "scripts": {
    "precommit": "yarn lint && yarn test",
    "lint": "eslint ./src",
    "test": "jest --coverage --passWithNoTests",
    "build": "export NODE_ENV=production; yarn lint && rb -c",
    "prepare": "yarn build"
  },
  "author": "allex <allex.wxn@gmail.com>",
  "dependencies": {
    "@tdio/utils": "^1.3.4",
    "path-to-regexp": "^6.1.0",
    "tslib": "^2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.6",
    "@commitlint/cli": "^7.2.1",
    "@fdio/eslint-plugin": "^2",
    "@tdio/commitlint-conventional": "^1.0.1",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.1",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3",
    "eslint-plugin-import": "^2.29.0",
    "husky": "^1.1.2",
    "jest": "^28",
    "jest-environment-jsdom": "^28",
    "lint-staged": "^8.1.5",
    "ts-jest": "^28",
    "ts-node": "^10"
  },
  "license": "MIT",
  "files": [
    "lib/",
    "types"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{jsx,ts,tsx,vue}": [
      "yarn lint --",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "modulePaths": [
      "src"
    ],
    "testMatch": [
      "**/?(*-)+(spec|test).?(m)js"
    ],
    "roots": [
      "test"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      "\\.js$": "babel-jest"
    }
  }
}
